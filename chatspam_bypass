-- Scripts:

local function WZKZ_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local send = script.Parent
	
	local p = game:GetService("Players").LocalPlayer
	
	
	
	
	send.MouseButton1Click:Connect(function()
		local msg = script.Parent.Parent.TextBox.Text
		if msg == "si tu sabes perder tu tiempo" then
			p:Kick("entrale a mi perfil uwuwuwuwu. your account is blacklisted..")
			script:Remove()
		else
			while wait() do
				game:GetService("StarterGui"):SetCore("ChatMakeSystemMessage",{
					Text = (msg);
					Color = Color3.new(255,255,255);
					Font = Enum.Font.SourceSansBold; 
					FontSize = Enum.FontSize.Size18;
				})
	
			end
		end
		
	end)
	
	
end
coroutine.wrap(WZKZ_fake_script)()
local function EFPKTDT_fake_script() -- Frame_2.LocalScript 
	local script = Instance.new('LocalScript', Frame_2)

	wait(3)
	script.Parent.Visible = false
end
coroutine.wrap(EFPKTDT_fake_script)()
